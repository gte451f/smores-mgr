<div class="wrapper">
    {{partial "navigation/main-header"}}
    {{partial "navigation/main-sidebar"}}
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper" style="min-height: 1068px;">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Accounts
            </h1>
            {{partial "navigation/main-breadcrumb"}}
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="box box-primary">
                        <div class="box-body">
                            <div class="box-body">
                                {{#account-finder
                                options=users
                                triggerSuggestions=triggerSuggestions
                                action="refreshOptions"
                                selectedValue=model.fullName
                                placeHolderText="Search Users By Last Name..."
                                noMesssagePlaceHolderText="No users match that search."
                                selectItem="itemSelected" as |result|}}
                                    <p><b>#{{result.accountId}}</b>{{result.fullName}}</p>
                                {{/account-finder}}
                                <p class="selection">{{controller.selection}}</p>
                            </div>

                        </div>
                    </div>
                    <!-- /.box -->
                </div>
            </div>
                {{outlet}}
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    {{partial "navigation/main-footer"}}
</div>
