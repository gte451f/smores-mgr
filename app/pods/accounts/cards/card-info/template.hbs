<div class="info-box bg-yellow">
    <span class="info-box-icon">{{fa-icon 'credit-card'}}</span>

    <div class="info-box-content">
        <span class="info-box-text">{{card.vendor}}</span>
        <span class="info-box-number">Expires: {{card.expirationMonth}}/{{card.expirationYear}}
        <span class="pull-right">
            #...{{card.number}}
        </span>
        </span>
        <div class="progress">
            <div class="progress-bar" style="width: 100%"></div>
        </div>
        <span class="progress-description">
            Added: {{moment-format card.createdOn 'MM-DD-YYYY'}}
        </span>
    </div>
    <!-- /.info-box-content -->
</div>